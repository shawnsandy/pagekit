{
    "name": "shawnsandy/pagekit",
    "type": "library",
    "description": "Laravel PageKit",
    "keywords": [
        "shawnsandy",
        "PageKit"
    ],
    "homepage": "https://github.com/shawnsandy/pagekit",
    "license": "MIT",
    "authors": [{
        "name": "Shawn Sandy",
        "email": "shawnsandy04@gmail.com",
        "homepage": "http://shawnsandy.design",
        "role": "Developer"
    }],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "~5.1",
        "jeroen-g/laravel-packager": "^1.5",
        "barryvdh/laravel-debugbar": "^2.2",
        "laravelcollective/html": "^5.2",
        "doctrine/dbal": "^2.5",
        "barryvdh/laravel-ide-helper": "^2.1",
        "spatie/laravel-permission": "^1.3",
        "spatie/geocoder": "^2.1",
        "mpociot/laravel-test-factory-helper": "^0.2.0",
        "laravel/socialite": "^2.0",
        "laracasts/utilities": "^2.1",
        "laracasts/flash": "^2.0",
        "laracasts/generators": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.6.0",
        "vinkla/hashids": "^2.2",
        "mews/purifier": "^2.0",
        "mcamara/laravel-localization": "^1.1",
        "greggilbert/recaptcha": "^2.1",
        "graham-campbell/exceptions": "^8.4",
        "arrilot/laravel-widgets": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ShawnSandy\\PageKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShawnSandy\\PageKit\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
